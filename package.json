{
  "name": "node-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Node.js starter project",
  "keywords": [
    "boilerplate",
    "nodejs",
    "starter"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/node-starter.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://github.com/invrs/node-starter#readme",
  "scripts": {
    "build": "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-styled-components": "^1.5.1",
    "enzyme": "3",
    "enzyme-adapter-react-16": "1",
    "fxtr": "1",
    "jest": "^22.4.3",
    "rimraf": "2"
  },
  "dependencies": {},
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testMatch": [
      "**/*Test.js"
    ]
  }
}
